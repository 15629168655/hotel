        this.cname = cname;
    }

    public String getCidcard() {
        return this.cidcard;
    }
    
    public void setCidcard(String cidcard) {
        this.cidcard = cidcard;
    }

    public String getCphone() {
        return this.cphone;
    }
    
    public void setCphone(String cphone) {
        this.cphone = cphone;
    }
   








